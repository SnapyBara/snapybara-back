  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:errors": "eslint \"{src,apps,libs,test}/**/*.ts\" --quiet",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json --runInBand --forceExit",
    "test:e2e:watch": "NODE_ENV=test jest --config ./test/jest-e2e.json --watch",
    "test:clear": "jest --clearCache",
    "sonar": "sonar-scanner",
    "sonar:local": "bash scripts/sonar-local.sh",
    "test:db-simple": "ts-node -r tsconfig-paths/register src/test-db-simple.ts",
    "test:google-places": "ts-node -r tsconfig-paths/register src/google-places/test-google-places.ts",
    "script:import-landmarks": "ts-node -r tsconfig-paths/register src/scripts/import-natural-landmarks.ts",
    "script:test-search": "ts-node -r tsconfig-paths/register src/scripts/test-search.ts",
    "script:clear-queue": "ts-node -r tsconfig-paths/register src/scripts/clear-overpass-queue.ts",
    "script:migrate-cloudinary": "ts-node -r tsconfig-paths/register src/scripts/migrate-to-cloudinary.ts"
